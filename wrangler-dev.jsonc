{
  // 개발 환경용 Wrangler 설정
  "name": "distributed-crawling-system-dev",
  "main": "src/index.js",
  "compatibility_date": "2024-01-01",
  
  // 개발 환경 Durable Objects
  "durable_objects": {
    "bindings": [
      {
        "name": "COORDINATOR",
        "class_name": "CrawlingCoordinator"
      },
      {
        "name": "WEBSOCKET_HANDLER",
        "class_name": "WebSocketHandler"
      }
    ]
  },
  
  // 개발 환경 KV namespaces (로컬 시뮬레이션)
  "kv_namespaces": [
    {
      "binding": "JOBS_KV",
      "id": "dev-jobs-kv",
      "preview_id": "dev-jobs-kv-preview"
    },
    {
      "binding": "AGENTS_KV",
      "id": "dev-agents-kv",
      "preview_id": "dev-agents-kv-preview"
    }
  ],
  
  // 개발 환경 변수
  "vars": {
    "ENVIRONMENT": "development",
    "LOG_LEVEL": "debug"
  },
  
  // 개발 서버 설정
  "dev": {
    "port": 8787,
    "local_protocol": "http",
    "ip": "localhost"
  },
  
  // 로컬 개발용 설정
  "miniflare": {
    "kv_persist": true,
    "durable_objects_persist": true
  }
}